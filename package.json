{
    "name": "cimonitor",
    "version": "4.0.0",
    "description": "Monitors all your projects CI automatically",
    "repository": "git@github.com:CIMonitor/CIMonitor.git",
    "license": "MIT",
    "author": {
        "name": "Rick van der Staaij",
        "url": "https://rick.nu"
    },
    "contributors": [
        "https://github.com/CIMonitor/CIMonitor/graphs/contributors"
    ],
    "scripts": {
        "start": "node app/backend/server.js",
        "build": "tsc && parcel build --dist-dir dashboard --no-source-maps frontend/index.html",
        "server": "nodemon --watch backend --exec ts-node backend/server.ts",
        "dashboard": "parcel watch --dist-dir dashboard --hmr-port 3031 frontend/index.html"
    },
    "dependencies": {
        "express": "^4.17.3"
    },
    "devDependencies": {
        "@types/node": "^17.0.18",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "nodemon": "^2.0.15",
        "parcel": "^2.3.2",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "styled-components": "^5.3.3",
        "ts-node": "^10.6.0",
        "typescript": "^4.5.5"
    }
}
